{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "source": "./index.html",
  "scripts": {
    "start": "parcel --port 8000",
    "build": "parcel build",
    "lint": "eslint --fix --ext .",
    "server": "../gradlew bootRun -p ../ --args='--spring.profiles.active=test' || exit 0",
    "serve": "servor dist index.html 8000 --reload",
    "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
    "codeceptjs": "codeceptjs run --steps",
    "test": "concurrently -s first -k \"npm run serve\" \"npm run codeceptjs\"",
    "ci": "concurrently -s first -k 'npm run server' 'npm run serve' 'sleep 30 && npm run codeceptjs:headless'",
    "watch:test": "jest --watchAll"
  },
  "author": "seedwhale",
  "license": "ISC",
  "devDependencies": {
    "@codeceptjs/configure": "^0.10.0",
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "buffer": "^5.7.1",
    "codeceptjs": "^3.3.7",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "parcel": "^2.8.3",
    "playwright": "^1.30.0",
    "process": "^0.11.10",
    "servor": "^4.0.2"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "concurrently": "^7.6.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-router-dom": "^6.11.1",
    "styled-components": "^5.3.6",
    "swr": "^2.0.3",
    "usehooks-ts": "^2.9.1"
  }
}
